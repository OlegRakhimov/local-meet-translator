# Copy this file to .env (DO NOT COMMIT .env) and fill your values.
# Then run scripts/windows/run-bridge.ps1 which can load .env into environment variables.

OPENAI_API_KEY=sk-your-openai-key-here
LOCAL_MEET_TRANSLATOR_PORT=8799
LOCAL_MEET_TRANSLATOR_TOKEN=your-random-token-here

# Models
OPENAI_TRANSCRIBE_MODEL=whisper-1
OPENAI_TEXT_MODEL=gpt-4o-mini

# Optional TTS (local playback)
ENABLE_TTS=false
OPENAI_TTS_MODEL=gpt-4o-mini-tts
OPENAI_TTS_VOICE=onyx
OPENAI_TTS_FORMAT=mp3
OPENAI_TTS_SPEED=1.0
